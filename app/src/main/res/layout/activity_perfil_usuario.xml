<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/corFundo">

    <View
        android:id="@+id/viFoto"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/custom_photo_place" />

    <TextView
        android:id="@+id/nome"
        android:layout_width="51dp"
        android:layout_height="30dp"
        android:layout_marginLeft="107dp"
        android:layout_marginTop="30dp"
        android:fontFamily="@font/alef"
        android:gravity="top"
        android:text="Nome"
        android:textColor="@color/white"
        android:textSize="18dp" />


    <EditText
        android:id="@+id/edtNome"
        android:layout_width="185dp"
        android:layout_height="30dp"
        android:layout_marginLeft="165dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/custom_edt"
        android:textColor="@color/white" />

    <TextView
        android:id="@+id/cpf"
        android:layout_width="48dp"
        android:layout_height="30dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="108dp"
        android:layout_marginTop="80dp"
        android:fontFamily="@font/alef"
        android:gravity="top"
        android:text="CPF"
        android:textColor="@color/white"
        android:textSize="18dp" />


    <EditText
        android:id="@+id/edtCpf"
        android:layout_width="185dp"
        android:layout_height="30dp"
        android:layout_marginLeft="165dp"
        android:layout_marginTop="80dp"
        android:background="@drawable/custom_edt"
        android:textColor="@color/white" />

    <TextView
        android:id="@+id/cidade"
        android:layout_width="59dp"
        android:layout_height="30dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="96dp"
        android:layout_marginTop="130dp"
        android:fontFamily="@font/alef"
        android:gravity="top"
        android:text="Cidade"
        android:textColor="@color/white"
        android:textSize="18dp" />


    <EditText
        android:id="@+id/edtCidade"
        android:layout_width="185dp"
        android:layout_height="30dp"
        android:layout_marginLeft="165dp"
        android:layout_marginTop="130dp"
        android:background="@drawable/custom_edt"
        android:textColor="@color/white" />

    <TextView
        android:id="@+id/area"
        android:layout_width="140dp"
        android:layout_height="30dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="180dp"
        android:fontFamily="@font/alef"
        android:gravity="top"
        android:text="Área de atuação"
        android:textColor="@color/white"
        android:textSize="18dp" />

    <EditText
        android:id="@+id/edtArea"
        android:layout_width="185dp"
        android:layout_height="30dp"
        android:layout_marginLeft="165dp"
        android:layout_marginTop="180dp"
        android:background="@drawable/custom_edt"
        android:textColor="@color/white" />

    <TextView
        android:id="@+id/email"
        android:layout_width="49dp"
        android:layout_height="30dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="106dp"
        android:layout_marginTop="230dp"
        android:fontFamily="@font/alef"
        android:gravity="top"
        android:text="Email"
        android:textColor="@color/white"
        android:textSize="18dp" />

    <EditText
        android:id="@+id/edtEmail"
        android:layout_width="185dp"
        android:layout_height="30dp"
        android:layout_marginLeft="165dp"
        android:layout_marginTop="230dp"
        android:background="@drawable/custom_edt"
        android:textColor="@color/white" />

    <TextView
        android:id="@+id/senha"
        android:layout_width="88dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="80dp"
        android:layout_marginTop="330dp"
        android:fontFamily="@font/alef"
        android:gravity="top"
        android:text="Descricao"
        android:textColor="@color/white"
        android:textSize="18dp" />

    <TextView
        android:id="@+id/senha2"
        android:layout_width="53dp"
        android:layout_height="30dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="103dp"
        android:layout_marginTop="280dp"
        android:fontFamily="@font/alef"
        android:gravity="top"
        android:text="Senha"
        android:textColor="@color/white"
        android:textSize="18dp" />

    <EditText
        android:id="@+id/edtSenha"
        android:layout_width="185dp"
        android:layout_height="30dp"
        android:layout_marginLeft="165dp"
        android:layout_marginTop="280dp"
        android:background="@drawable/custom_edt"
        android:textColor="@color/white" />

    <EditText
        android:id="@+id/edtDescricao"
        android:layout_width="186dp"
        android:layout_height="61dp"
        android:layout_marginLeft="165dp"
        android:layout_marginTop="330dp"
        android:background="@drawable/custom_edt"
        android:textColor="@color/white" />

    <View
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="406dp"
        android:background="#B5CEFF"
        android:focusable="false" />

    <TextView
        android:layout_width="245dp"
        android:layout_height="25dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="414dp"
        android:text="VAGAS APLICADAS"
        android:textColor="#FFFFFF"
        android:textSize="20dp" />

    <LinearLayout
        android:layout_width="355dp"
        android:layout_height="298dp"
        android:layout_marginTop="450dp">

    <GridView
        android:id="@+id/vagaGridView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:horizontalSpacing="10dp"
        android:numColumns="1"
        android:verticalSpacing="20dp" />

    </LinearLayout>

</RelativeLayout>